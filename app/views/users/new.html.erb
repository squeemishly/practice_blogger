<h1>Create an Account</h1>

  <%= form_for(@user) do |f| %>

    <%= f.label :first_name %>
    <%= f.text_field :first_name %>

    <%= f.label :last_name %>
    <%= f.text_field :last_name %><br>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.text_field :password %>

    <%= f.label :email %>
    <%= f.text_field :email %><br>

    <%= f.submit "Create Account" %>
  <% end %>
